// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Drug {
  id          String   @id @default(uuid())
  code        String
  genericName String
  brandName   String
  company     String
  launchDate  DateTime
}
